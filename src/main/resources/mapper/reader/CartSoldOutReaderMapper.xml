	<select id="selectCartSoldOutList" parameterType="java.lang.String" resultType="TmSpbkEntity">
        SELECT /* CartSoldOutReaderMapper.selectCartSoldOutList */
		    spbk_id,      mmbr_num,       mmbr_ysno
        FROM
            eos.tm_spbk
        WHERE mmbr_num = #{mmbrNum}
    </select>

</mapper>

